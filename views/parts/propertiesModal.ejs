<div id="properties-modal<%= index %>"
  class="modal hidden fixed inset-0 z-50 overflow-auto bg-black bg-opacity-50 flex justify-center items-center">
  
  <div class="modal-content bg-white rounded-lg p-8 md:flex flex-col w-1/4">
    <div class="flex justify-between items-center mb-4">
      <span class="text-center text-[#0CC0DF] font-medium text-xl">File Details</span>
      <button class="text-2xl text-[#0CC0DF]" onclick="closePropertiesModal(<%= index %>)">&times;</button>
    </div>
    <div id="entity-details<%= index %>" class="md:flex md:flex-col">
      
    </div>
  </div>
</div>
